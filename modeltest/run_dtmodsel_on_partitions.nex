#NEXUS
begin paup;
	execute scar_nd1131003.haps.nex;
	charset ND1pos1 = 1-.\3;
	charset ND1pos2 = 2-.\3;
	charset ND1pos3 = 3-.\3;
end;

begin python;
   import shutil
   subsets = ['ND1pos1', 'ND1pos2', 'ND1pos3']
   for s in subsets:
       paup.cmd('include %s/only;' % s)
       paup.cmd('execute DT_ModSel_PAUP;')
       shutil.move('tree_file.tre', s + "_tree_file.tre")
       shutil.move('score_file', s + "_score_file")
end;
